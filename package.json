    "water-polo-men",
    "water-polo-women",
    "men's-water-polo",
    "women's-water-polo",
    "basketball",
    "ncaa-w",
    "basketball-men",
    "basketball-women",
    "mens-basketball",
    "women's-basketball",
    "men's-soccer",
    "women's-soccer",
    "soccer-men",
    "soccer-women",
    "mens-ice-hockey",
    "women's-ice-hockey",
    "field-hockey",
    "men's-ice-hockey",
    "women's-ice-hockey",
    "ice-hockey-men",
    "ice-hockey-women",
    "baseball",
