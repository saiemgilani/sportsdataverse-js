{
  "name": "sportsdataverse",
  "version": "2.0.4",
  "type": "module",
  "description": "Node.js client which retrieves sports data from the ESPN API as well as from the NCAA website, with support for NBA, NFL, NHL, MLB, WNBA, men's and women's college basketball, and college football.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "chai": "^5.0.3",
    "cheerio": "^1.0.0-rc.12",
    "decode-html": "^2.0.0",
    "tabletojson": "^4.1.1"
  },
  "devDependencies": {
    "docdash": "latest",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.1",
    "mocha": "^10.3.0",
    "mochawesome": "^7.1.3",
    "should": "^13.2.3",
    "taffydb": "^2.7.3",
    "underscore": ">=1.13.6"
  },
  "scripts": {
    "test": "mocha --timeout 10000 --reporter mochawesome",
    "start": "node server.js",
    "document": "npm run md-all && npm run md-readme && npm run readme",
    "jsdoc-build": "jsdoc app -r -c conf.json -d docs",
    "jsdoc2md": "node_modules/.bin/jsdoc2md",
    "readme": "jsdoc2md --template md/README.hbs --files app/services/cfb.service.js app/services/mbb.service.js app/services/mlb.service.js app/services/nba.service.js app/services/ncaa.service.js app/services/nfl.service.js app/services/nhl.service.js app/services/wbb.service.js app/services/wnba.service.js > README.md",
    "md-readme": "jsdoc2md --template md/intro.hbs --files app/services/cfb.service.js app/services/mbb.service.js app/services/mlb.service.js app/services/nba.service.js app/services/ncaa.service.js app/services/nfl.service.js app/services/nhl.service.js app/services/wbb.service.js app/services/wnba.service.js > docs/docs/intro.md",
    "md-cfb": "jsdoc2md app/services/cfb.service.js > docs/docs/cfb.md",
    "md-mbb": "jsdoc2md app/services/mbb.service.js > docs/docs/mbb.md",
    "md-mlb": "jsdoc2md app/services/mlb.service.js > docs/docs/mlb.md",
    "md-nba": "jsdoc2md app/services/nba.service.js > docs/docs/nba.md",
    "md-ncaa": "jsdoc2md app/services/ncaa.service.js > docs/docs/ncaa.md",
    "md-nfl": "jsdoc2md app/services/nfl.service.js > docs/docs/nfl.md",
    "md-nhl": "jsdoc2md app/services/nhl.service.js > docs/docs/nhl.md",
    "md-tennis": "jsdoc2md app/services/tennis.service.js > docs/docs/tennis.md",
    "md-wbb": "jsdoc2md app/services/wbb.service.js > docs/docs/wbb.md",
    "md-wnba": "jsdoc2md app/services/wnba.service.js > docs/docs/wnba.md",
    "md-all": "npm run md-cfb && npm run md-mbb && npm run md-nba && npm run md-ncaa && npm run md-nfl && npm run md-nhl && npm run md-wbb && npm run md-wnba && npm run md-mlb && npm run md-tennis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sportsdataverse/sportsdataverse-js.git"
  },
  "keywords": [
    "sports",
    "nba",
    "nfl",
    "nhl",
    "mlb",
    "wnba",
